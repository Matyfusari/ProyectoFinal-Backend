<html lang='en'>
    <head>
        <link rel='stylesheet' href='../logIn.css' />
    </head>
    <body>
        <form class='logInDiv' method='post' action='/'>
            <label for='username'>Ingresa tu email</label>
            <input type='email' id='userEmail' name='username' />
            <label for='password'>Ingresa tu contraseña</label>
            <input type='password' id='userPassword' name='password' />
            <input type='submit' value='Iniciar Sesión' id='submitSession' />
        </form>
        <input id='goToSignIn' type='button' value='SignIn' />
        <script src='../logIn.js'></script>
    </body>

</html>