<html lang='en'>
    <head>
        <link rel='stylesheet' href='../signIn.css' />
    </head>
    <body>
        <form class='singInDiv' method='post' action='signIn'>
            <label for='username'>Ingresa tu email</label>
            <input type='email' id='userEmail' name='username' />
            <label for='password'>Ingresa tu contraseña</label>
            <input type='password' id='userPassword' name='password' />
            <label for='nombre'>Ingresa tu Nombre</label>
            <input type='text' id='userName' name='nombre' />
            <label for='direccion'>Ingresa tu Dirección</label>
            <input type='text' id='userAdress' name='direccion' />
            <label for='edad'>Ingresa tu Edad</label>
            <input type='number' id='userAge' name='edad' />
            <label for='telefono'>Ingresa tu número de teléfono</label>
            <input type='number' id='userPhone' name='telefono' />
            <input type='submit' value='Registrarte' id='submitSession' class='submit' />
        </form>
        <input id='goToLogIn' type='button' value='LogIn' />
        <script src='../signIn.js'></script>
    </body>

</html>